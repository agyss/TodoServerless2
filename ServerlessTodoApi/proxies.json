{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
					"routetobase": {
						"matchCondition": {
							"route": "/",
							"methods": [
								"GET"
							]
						},
						"backendUri": "http://localhost:10000/devstoreaccount1/todoserverless/index.html"
					},
					"jsproxy": {
						"matchCondition": {
							"route": "/js/{*restOfPath}",
							"methods": [
								"GET"
							]
						},
						"backendUri": "http://localhost:10000/devstoreaccount1/todoserverless/js/{restOfPath}"
					}
    }
}